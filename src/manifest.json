{
  "manifest_version": 3,
  "name": "RAGnificent",
  "version": "v0.1",
  "description": "Query any webpage for fast results with on-device AI.",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}
